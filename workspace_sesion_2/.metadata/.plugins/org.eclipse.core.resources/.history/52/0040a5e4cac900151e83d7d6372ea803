package g2_1;

public class EjecutorG2_1 {
	
	public static int hexDigToDec (char otraLetra)
	{
		int ret=-1;		
		boolean error=!(
						(unaLetra>='0' && unaLetra<='9') &&
						(unaLetra>='a' && unaLetra<='f') &&
						(unaLetra>='A' && unaLetra<='F')
						);
				
				
	
		
		
		return ret;
	}

}
