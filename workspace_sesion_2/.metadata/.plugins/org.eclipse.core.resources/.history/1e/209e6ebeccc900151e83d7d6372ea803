package g2_1;

public class EjecutorG2_1 {
	
	public static int hexDigToDec (char unaLetra)
	{
		int ret=-1;		
		
		//convierto todo a minusculas
		if (unaLetra>='A' && unaLetra<='F')
			unaLetra=(char)('a'+unaLetra-'f');
			
		
		
		boolean error=!(
						(unaLetra>='0' && unaLetra<='9') &&
						(unaLetra>='a' && unaLetra<='f')						
						);
				
				
	
		
		
		return ret;
	}

}
