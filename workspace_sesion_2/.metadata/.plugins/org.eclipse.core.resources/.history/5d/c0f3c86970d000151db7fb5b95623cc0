package g2_3;

public class EjecutorG2_3 {
	
		public int hexToDec(String palabra)
		{
			int unNumero=0;
			int i=0;
			int ptro=0;
			
			while ((i<palabra.length()))
			{
				ptro=unHex.hexDigToDec(palabra.charAt(i));
				if (ptro==-1) return -1;
				unNumero*=16;
				unNumero+=ptro;
			}
			
			return unNumero;
		}
}
